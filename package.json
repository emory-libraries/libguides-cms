{
  "name": "libguides-cms",
  "version": "1.0.0",
  "description": "Style overrides for LibGuides CMS",
  "scripts": {
    "watch": "sass --watch src/scss:dist/css --sourcemap=none",
    "build": "sass --update src/scss:dist/css --sourcemap=none",
    "deploy": "npm run build && dploy libguides"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emory-libraries/libguides-cms.git"
  },
  "keywords": [
    "libguides",
    "css",
    "scss"
  ],
  "author": "Lauren Hamel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/emory-libraries/libguides-cms/issues"
  },
  "homepage": "https://github.com/emory-libraries/libguides-cms#readme",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "grunt": "^1.0.2",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-ssh-deploy": "^0.4.1"
  }
}
